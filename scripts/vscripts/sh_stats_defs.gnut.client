global int MAX_BIT_FIELD_SIZE = 32
const int BITFIELD_SAFETY_WARNING_THRESHOLD = 3

void function ShStatsDefs_Init()
{
	FileLevel_Lifetime fileStruct
	file = fileStruct

	
	

	foreach ( int index, string guid in GetAllCharacterGUIDStringsForStats() )
	{
		Assert( !( guid in file.characterGUIDToBitmask ), printf("Duplicate character GUID found: %s", guid) )
		Assert( index < MAX_BIT_FIELD_SIZE, "Bitfield requested is too big! -- Please see Jira: https://jiratf.rspn.ad.ea.com/browse/R5DEV-587155" )
		Assert( index < MAX_BIT_FIELD_SIZE - BITFIELD_SAFETY_WARNING_THRESHOLD, "Bitfield is nearing maximum supported size!! -- Please see Jira: https://jiratf.rspn.ad.ea.com/browse/R5DEV-587155")

#if DEV
			printf("adding character guid %s to bitmask index %d", guid, index)
#endif

		file.characterGUIDToBitmask[ guid ] <- 1 << index
	}

#if DEV
		
		print("CHARACTER_GUID_TO_BITMASK_MAP contents:\n")
		foreach ( string guid, int bitmask in file.characterGUIDToBitmask )
		{
			printf("guid: %s | bitmask: %d\n", guid, bitmask)
		}
		printf("total entries: %d/%d\n", file.characterGUIDToBitmask.len(), MAX_BIT_FIELD_SIZE)
#endif
}

table< string, int > function StatsDefs_GetCharacterGUIDToBitmaskMap()
{
	return file.characterGUIDToBitmask
}