void function SubstituteBattlePassRewardsForUserRestrictions( entity player, BattlePassReward reward )
{
	bool isFreeReward = ( !reward.isPremium && !reward.isElite )

	Assert( reward.rewardTier != eBattlePassV2RewardTier.UNSPECIFIED )
	isFreeReward = isFreeReward && ( reward.rewardTier == eBattlePassV2RewardTier.FREE )


	if ( isFreeReward ) 
			return

	bool lootBoxesAllowed = !GRX_IsOfferRestricted( player )
	bool isRewardPackType = ( ItemFlavor_GetType( reward.flav ) == eItemType.account_pack )

	if ( !isRewardPackType )
	{
		return
	}

	if ( !lootBoxesAllowed )
	{
		switch ( ItemFlavor_GetAsset( reward.flav ) )
		{
			case $"settings/itemflav/pack/cosmetic_rare.rpak":
				reward.flav = GetItemFlavorByAsset( $"settings/itemflav/grx_currency/crafting.rpak" )
				reward.quantity = 30
				break

			case $"settings/itemflav/pack/cosmetic_epic.rpak":
				reward.flav = GetItemFlavorByAsset( $"settings/itemflav/grx_currency/crafting.rpak" )
				reward.quantity = 200
				break

			case $"settings/itemflav/pack/cosmetic_legendary.rpak":
				reward.flav = GetItemFlavorByAsset( $"settings/itemflav/grx_currency/crafting.rpak" )
				reward.quantity = 600
				break

			default:
				reward.flav = GetItemFlavorByAsset( $"settings/itemflav/grx_currency/crafting.rpak" )
				reward.quantity = 30
		}
	}
}




