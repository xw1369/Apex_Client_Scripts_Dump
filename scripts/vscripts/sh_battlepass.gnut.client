array<BattlePassReward> function GetBattlePassV2Rewards( ItemFlavor pass, int levelIndex, int rewardTierTarget, entity player = null, bool allRewards = false, int allRewardsMinimumLevel = -1 )
{
	
	
	Assert( ItemFlavor_GetType( pass ) == eItemType.battlepass )
	Assert( levelIndex >= 0, "Level must be >= 0" )

	bool isV2Pass = BattlePassV2_IsV2BattlePass( pass )
	if ( !isV2Pass )
	{
		Assert( rewardTierTarget == eBattlePassV2RewardTier.FREE || rewardTierTarget == eBattlePassV2RewardTier.PREMIUM, "Only FREE & PREMIUM tiers are supported on V1 assets." )
	}
	else
	{
		Assert( GetEnumString( "eBattlePassV2RewardTier", rewardTierTarget ) in eBattlePassV2RewardTier )
	}

	if ( ItemFlavor_GetType( pass ) != eItemType.battlepass || levelIndex < 0)
		return []

	var scheduleDT = isV2Pass ? BattlePassV2_GetScheduleDatatable( pass ) : BattlePass_GetScheduleDatatable( pass )

	array<BattlePassReward> rewardList = []

	int rowIndex = 0;
	int currentLevel = 0;

	int numRows = GetDataTableRowCount( scheduleDT )

	while ( rowIndex < numRows )
	{
		
		asset rewardAsset 	= GetDataTableAsset( scheduleDT, rowIndex, GetDataTableColumnByName( scheduleDT, "reward" ) )
		int rewardQty 		= GetDataTableInt( scheduleDT, rowIndex, GetDataTableColumnByName( scheduleDT, "rewardQty" ) )
		currentLevel		= GetDataTableInt( scheduleDT, rowIndex, GetDataTableColumnByName( scheduleDT, "levelIndex" ) )
		bool premium 	  	= GetDataTableBool( scheduleDT, rowIndex, GetDataTableColumnByName( scheduleDT, "premium" ) )

		
		int rewardTier 		= isV2Pass ? GetDataTableInt( scheduleDT, rowIndex, GetDataTableColumnByName( scheduleDT, "rewardTier" ) ) : eBattlePassV2RewardTier.UNSPECIFIED
		bool isChaseReward 	= isV2Pass ? GetDataTableBool( scheduleDT, rowIndex, GetDataTableColumnByName( scheduleDT, "isChaseReward" ) ) : false

		
		if ( rewardTier == eBattlePassV2RewardTier.UNSPECIFIED )
		{
			
			
			rewardTier = premium ? eBattlePassV2RewardTier.PREMIUM : eBattlePassV2RewardTier.FREE
		}

		if ( currentLevel < levelIndex && !allRewards )
		{
			rowIndex += 1
			continue
		}

		if ( currentLevel < allRewardsMinimumLevel && allRewards  )
		{
			rowIndex += 1
			continue
		}

		if ( currentLevel > levelIndex )
			break

		if ( rewardTierTarget == rewardTier || ( allRewards && rewardTier <= rewardTierTarget ) )
		{
			if ( rewardAsset != $"" )
			{
				if ( IsValidItemFlavorSettingsAsset( rewardAsset ) )
				{
					BattlePassReward reward

					reward.flav = GetItemFlavorByAsset( rewardAsset )
					string overrideRefGUIDString = GetCurrentPlaylistVarString( format( "%s_level_%d_override_ref", ItemFlavor_GetGUIDString( pass ), levelIndex ), "" )
					if ( overrideRefGUIDString != "" )
						reward.flav = GetItemFlavorByGUID( ConvertItemFlavorGUIDStringToGUID( overrideRefGUIDString ) )

					reward.quantity = rewardQty
					int overrideQty = GetCurrentPlaylistVarInt( format( "%s_level_%d_override_qty", ItemFlavor_GetGUIDString( pass ), levelIndex ), -1 )
					if ( overrideQty != -1 )
						reward.quantity = overrideQty

					reward.level = levelIndex
					reward.isPremium = premium


						reward.rewardTier = rewardTier
						reward.isChaseReward = isChaseReward


					SubstituteBattlePassRewardsForUserRestrictions( player, reward )

					rewardList.append( reward )
				}
			}
		}

		rowIndex++
	}

	return rewardList
}



