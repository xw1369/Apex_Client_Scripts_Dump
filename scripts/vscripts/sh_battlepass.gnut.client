					reward.quantity = rewardQty
					int overrideQty = GetCurrentPlaylistVarInt( format( "%s_level_%d_override_qty", ItemFlavor_GetGUIDString( pass ), levelIndex ), -1 )
					if ( overrideQty != -1 )
						reward.quantity = overrideQty


