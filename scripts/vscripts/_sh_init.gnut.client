void function SV_CL_Shared_Init()
{

	if ( !IsLobby() )
		ClMinimap_Pre_Init()


#if DEV
		PrintDebugTimeDetails()
#endif





	GameModes_Init()
	Settings_Init()


		Crafting_Init()






	if ( IsLobby() )
	{
		Shared_Lobby_Init()
		PlayerWaypoints_Init()
		return
	}

	SV_CL_Init()






		Client_Init()


	RemoteFunctions_Init_Finish()




} 


